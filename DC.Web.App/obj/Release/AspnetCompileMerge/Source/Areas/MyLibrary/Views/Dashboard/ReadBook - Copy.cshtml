                var val = parseInt($("#idslider").val());
                val = val + 1;
                $("#idslider").val(val)
                ZoomInOut(val)
            })
            $("#Zoom-Out").click(function () {
                var val = parseInt($("#idslider").val());
                val = val - 1;
                $("#idslider").val(val)
                ZoomInOut(val)
            })

        })

